{
  "name": "Apple Certificate Info - Tenant",
  "style": "Tenant",
  "Fields": [
    {
      "name": "appleCertificate",
      "UseExistingInfo": false,
      "FrontendFields": [
        {
          "name": "account",
          "value": "appleCertificate.appleIdentifier",
          "formatter": "string"
        },
        {
          "name": "expiration",
          "value": "appleCertificate.expirationDateTime"
        }
      ],
      "StoreAs": "JSON",
      "ExtractFields": [
        "appleIdentifier",
        "expirationDateTime"
      ],
      "Command": "Get-CIPPAppleCertificateInfo",
      "URL": "https://graph.microsoft.com/beta/deviceManagement/applePushNotificationCertificate"
    }
  ]
}
